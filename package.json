{
  "name": "movies-site",
  "version": "1.0.0",
  "description": "Website for tracking movie watchlists",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon --delay 500ms -q dist/index.js\"",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nkeil/movies-site.git"
  },
  "keywords": [
    "movie"
  ],
  "author": "Nicolas Keil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkeil/movies-site/issues"
  },
  "homepage": "https://github.com/nkeil/movies-site#readme",
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "gts": "^3.1.1"
  }
}
